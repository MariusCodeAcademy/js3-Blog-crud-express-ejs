<%- include('./partials/head.ejs') %>
<!-- ikeliam bendra navigacijos faila -->
<%- include('./partials/nav.ejs') %>

<h2>Single page template</h2>
<section class="jumbotron text-center" data-post-id="<%= post._id %>">
  <div class="container">
    <h1><%= post.title %></h1>
    <h4><%= post.author %> </h4>
    <p class="lead text-muted"><%= post.body %> </p>
    <p class="lead text-muted"><%= post.updatedAt.toLocaleString() %> </p>
    <p>
      <a href="#" class="btn btn-primary my-2">Edit</a>
      <button href="#" class="btn btn-danger my-2">Delete</button>
    </p>
  </div>
</section>

<!-- ikeliam bendra footer -->
<%- include('./partials/footer.ejs') %>
