<% if (locals.feedback && feedback.msg === 'created') { %>
    <%- include('_alert', {className: 'success', action: 'Created, Yoy'}) %> 
<% } %> 

<% if (locals.feedback && feedback.msg === 'deleted') { %>
    <%- include('_alert', {className: 'danger', action: 'Deleted, Sad'}) %> 
<% } %> 

<% if (locals.feedback && feedback.msg === 'updated') { %>
    <%- include('_alert', {className: 'info', action: 'Updated, Yeee'}) %> 
<% } %>
